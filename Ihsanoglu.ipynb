    "from urllib import request\n",
    "import os\n",
    "import re\n",
    "import pandas as pd\n",
    "import csv\n",
    "import numpy as np\n",
    "\n",
    "regex_pattern = '([0-9]{1,3})\\.\\s*([A-Z.\\s,ŞÇÜÛİÖÎĞÂ‘’\\'\\/-]+)\\(.+\\)'\n",
    "    #this is the pattern that we are trying to match\n",
    "authors = []\n",
    "text = ''\n",
    "with open (\"OSMANLI COĞRAFYA LİTERATÜRÜ TARİHİ 1.CİLT 2000.txt\", \"r\") as f:\n",
    "    text = f.read()\n",
    "    lst_authors = []\n",
    "    authors = re.findall (regex_pattern, text)\n",
    "    for a in authors:\n",
    "        author = '%s_%s' % a\n",
    "        if author[len(author)-1] == ' ':\n",
    "            author = author[0:len(author)-1]\n",
    "        author = author.replace(' ','_')\n",
    "        author = author.replace('-','')\n",
    "        author = author.replace('.','')\n",
    "        author = author.replace(',','')\n",
    "        author = author.replace('‘','')\n",
    "        lst_authors.append(author)"
   ]
