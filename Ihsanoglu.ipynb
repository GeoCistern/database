    "     \n",
    "    dates += re.findall(regex_hicri, metadata)\n",
    "    dates += re.findall(regex_baski, metadata)\n",
    "    dates += re.findall(regex_century, metadata)\n",
    "    dates += re.findall(regex_month, metadata)\n",
    "    \n",
    "    dates_4digit = re.findall(regex_4digit, metadata)\n",
    "    for i in range(len(dates_4digit)):\n",
    "        is_inside = False\n",
    "        for date in dates:\n",
    "            if dates_4digit[i].lstrip() in date:\n",
    "                is_inside = True\n",
    "                break\n",
    "        if is_inside == False:\n",
    "            dates.append(dates_4digit[i].lstrip())\n",
