    "    regex_baski = \"([0-9]{3,4}\\/[0-9]{4})\"\n",
    "    regex_4digit = \"((?:(?<!nr\\.)(?<!nr\\,)(?<!s\\.)(?<!s\\,)(?<!I\\,)(?<!II\\,)(?<!III\\,)(?<!IV\\,)(?<!V\\,))(?:\\s)[0-9]{4}(?![0-9]))\"\n",
    "    regex_century = \"(H\\.\\s? [XIV]+)\"\n",
    "    regex_month =\"((?:[Oo]cak|[ŞSşs]ubat|[Mm]art|[Nn]isan|[Mm]ayıs|[Hh]aziran|[Tt]emmuz|[Aa]ğustos|[Ee]yl(?:[üu]|[i]{2})l|[Ee]kim|[Kk]asım|[Aa]ralık|[Rr]umi|[Tt]eşrin-i [Ee]vvel|[Kk][âa]n[ûu]n-ı [Ss][âa]n[îiı]|[Tt]eşrin-i [Ss][âa]n[îiı]|[Kk][âa]n[ûu]n-ı [Ee]vvel|[Zz]ilkade|[Mm]uharrem|[ŞSşs]aban|[Rr]eceb|[Cc]emaziyelevvel|[Cc]emaziyelahir|[Rr]ebi(?:[üu]|[i]{2})lahir|[Ss]afer|[Rr]ebi(?:[üu]|[i]{2})levvel|[Rr]amazan|[Zz]ilhicce|[Şş]evval)\\s?[0-9]{3,4})\"\n",
