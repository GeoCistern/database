   "source": [
    "dict_authors_to_titles_to_meta = [] # dict (author : dict (titles : metadata)) \n",
    "\n",
    "str_textfile = ''\n",
    "lst_titles_curauth = []\n",
    "\n",
    "for i in range(len(author_text_file_paths_vol1)):\n",
    "    dict_author_to_meta = {} # dict (titles : metadata) (list has nusha, baski, neither or both)\n",
    "    with open (author_text_file_paths_vol1[i][1]) as f:\n",
    "        str_textfile = f.read()\n",
    "        lst_titles_curauth = re.findall(regex_work_titles, str_textfile)\n",
    "        lst_titles_indices = []\n",
    "        for title in lst_titles_curauth:\n",
    "            lst_titles_indices.append(str_textfile.find(title))\n",
    "        \n",
    "        lst_nusha_data = re.findall(regex_nusha, str_textfile)\n",
    "        lst_nusha_indices = []\n",
    "        for nusha in lst_nusha_data:\n",
    "            lst_nusha_indices.append(str_textfile.find(nusha))\n",
    "        \n",
    "        lst_baski_data = re.findall(regex_baski, str_textfile)\n",
    "        lst_baski_indices = []\n",
    "        for baski in lst_baski_data:\n",
    "            lst_baski_indices.append(str_textfile.find(baski))\n",
    "            \n",
    "        \n",
    "        # make copies of the lst_indices\n",
    "        \n",
    "        curr_author_metadata = {}\n",
    "        title_metadata = {}\n",
    "        \n",
    "        for j in range(len(lst_titles_indices)):\n",
    "            lst_metadata = []\n",
    "            curr_nusha = \"\"\n",
    "            curr_baski = \"\"\n",
    "            \n",
    "            index_curr_title = lst_titles_indices[j]\n",
    "            index_next_title = 0\n",
    "            if j == len(lst_titles_indices) - 1:\n",
    "                index_next_title = len(str_textfile) -1\n",
    "            else:\n",
    "                index_next_title = lst_titles_indices[j+1]\n",
    "            # loop through Nushas\n",
    "            for n_i in range(len(lst_nusha_indices)):\n",
    "                if lst_nusha_indices[n_i] > index_curr_title and lst_nusha_indices[n_i] < index_next_title:\n",
    "                    # add that nusha to the data structure\n",
    "                    curr_nusha = lst_nusha_data[n_i]\n",
    "                    # remove lst_index for nusha\n",
    "                    lst_nusha_data.pop(n_i)\n",
    "                    lst_nusha_indices.pop(n_i)\n",
    "                    break\n",
    "                \n",
    "            # loop through Baskis\n",
    "            for b_i in range(len(lst_baski_indices)):\n",
    "                if lst_baski_indices[b_i] > index_curr_title and lst_baski_indices[b_i] < index_next_title:\n",
    "                    # add that nusha to the data structure\n",
    "                    curr_baski = lst_baski_data[b_i]\n",
    "                    # remove lst_index for nusha\n",
    "                    lst_baski_data.pop(b_i)\n",
    "                    lst_baski_indices.pop(b_i)\n",
    "                    break\n",
    "                    \n",
    "            \n",
    "            lst_metadata = [curr_nusha, curr_baski]\n",
    "            \n",
    "            \n",
    "\n",
    "            title_metadata[lst_titles_curauth[j]] = lst_metadata # list with empty strings if title has no nushas or baskis\n",
    "            \n",
    "        dict_author_to_meta[author_text_file_paths_vol1[i][0] + \"_with_titles\"] = title_metadata\n",
    "        \n",
    "        # curr_no_title_metadata = {}\n",
    "        \n",
    "        # nushas = [12, 34, 65, 109]\n",
    "        # baskis = [13, 26, 55, 110, 114, 167, 189]\n",
    "        no_title_n_i = 0\n",
    "        no_title_b_i = 0\n",
    "        merged_lst = []\n",
    "        \n",
    "  \n",
    "        while no_title_n_i != len(lst_nusha_indices) and no_title_b_i != len(lst_baski_indices):\n",
    "            if lst_nusha_indices[no_title_n_i] < lst_baski_indices[no_title_b_i]:\n",
    "                merged_lst.append((lst_nusha_data[no_title_n_i], lst_nusha_indices[no_title_n_i]))\n",
    "                no_title_n_i += 1\n",
    "            else: \n",
    "                merged_lst.append((lst_baski_data[no_title_b_i], lst_baski_indices[no_title_b_i]))\n",
    "                no_title_b_i += 1\n",
    "        \n",
    "        if no_title_n_i == len(lst_nusha_indices):\n",
    "            while no_title_b_i != len(lst_baski_indices):\n",
    "                merged_lst.append((lst_baski_data[no_title_b_i], lst_baski_indices[no_title_b_i]))\n",
    "                no_title_b_i += 1\n",
