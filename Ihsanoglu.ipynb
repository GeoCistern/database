    "#for books\n",
    "\n",
    "dict_books_to_loc = {} # {atlas title: list of locations} #there is no biblio information for anon_atlas\n",
    "\n",
    "str_textfile = ''\n",
    "\n",
    "loc_and_num_words = {}\n",
    "for loc in lst_placenames:\n",
    "    curr_location = loc.split()\n",
    "    loc_and_num_words[loc] = len(curr_location)\n",
    "    \n",
    "for i in range(len(anon_books_and_paths)):\n",
    "    with open (anon_books_and_paths[i][1]) as f:\n",
    "        str_textfile = f.read()\n",
    "        title_book =  anon_books_and_paths[i][0]\n",
    "        \n",
    "        lst_book_anon = re.findall(regex_work_titles, str_textfile)\n",
    "        \n",
    "        book_index = str_textfile.find(lst_book_anon[0])\n",
    "    \n",
    "        lst_nusha_data = re.findall(regex_nusha, str_textfile)\n",
    "        lst_nusha_indices = []\n",
    "        for nusha in lst_nusha_data:\n",
    "            lst_nusha_indices.append(str_textfile.find(nusha))\n",
    "        \n",
    "        lst_baski_data = re.findall(regex_baski, str_textfile)\n",
    "        lst_baski_indices = []\n",
    "        for baski in lst_baski_data:\n",
    "            lst_baski_indices.append(str_textfile.find(baski))\n",
    "        \n",
    "        # per title\n",
    "        bool_nusha_title = True\n",
    "        bool_baski_title = True\n",
    "        content_curtitle = ''\n",
    "        if len(lst_nusha_indices) == 0:\n",
    "            bool_nusha_title = False\n",
    "        if len(lst_baski_indices) == 0:\n",
    "            bool_baski_title = False\n",
    "\n",
    "        \n",
    "        n_i = 0\n",
    "        b_i = 0\n",
    "        \n",
    "        if bool_nusha_title:\n",
    "            while lst_nusha_indices[n_i] < book_index and len(lst_nusha_indices) > n_i + 1:\n",
    "                n_i += 1  \n",
    "        if bool_baski_title:       \n",
    "            while lst_baski_indices[b_i] < book_index and len(lst_baski_indices) > b_i + 1:\n",
    "                b_i += 1\n",
    "\n",
    "                \n",
    "        if bool_nusha_title and bool_baski_title:\n",
    "            # print(\"both nusha and baski\")\n",
    "            if lst_nusha_indices[n_i] < lst_baski_indices[b_i]:\n",
    "                content_curtitle = str_textfile[book_index:lst_nusha_indices[n_i]]\n",
    "            else:\n",
    "                content_curtitle = str_textfile[book_index:lst_baski_indices[b_i]]\n",
    "\n",
    "        elif not bool_nusha_title and bool_baski_title:\n",
    "            # print(\"only baski\")  \n",
    "            content_curtitle = str_textfile[book_index:lst_baski_indices[b_i]]\n",
    "        elif not bool_baski_title and bool_nusha_title:\n",
    "            # print(\"only Nusha\")\n",
    "            content_curtitle = str_textfile[book_index:lst_nusha_indices[n_i]]\n",
    "        #no nusha or baski\n",
    "        else:\n",
    "            # print(\"goes here none\")\n",
    "            content_curtitle = str_textfile\n",
    "            \n",
    "        # print(content_curtitle)\n",
    "        \n",
    "        locations_in_content = []\n",
    "        lst_words_in_content = []\n",
    "\n",
    "        # Get content for each title \n",
    "\n",
    "        for loc in loc_and_num_words:\n",
    "            if loc.find(\"-\") != -1:\n",
    "                lst_words_in_content = content_curtitle.replace('\\t', ' ').replace('\\n', ' ').split()\n",
    "            else:\n",
    "                lst_words_in_content = content_curtitle.replace('\\t', ' ').replace('\\n', ' ').replace('-', ' ').split()\n",
    "            n_gram_length = loc_and_num_words[loc]\n",
    "            lst_n_grams_in_content = []\n",
    "            for w in range(len(lst_words_in_content) - n_gram_length + 1):\n",
    "                n_gram = \"\"\n",
    "                for n in range(n_gram_length):\n",
    "                    n_gram += lst_words_in_content[w + n] + \" \"\n",
    "                # remove spaces from the end \n",
    "                n_gram = n_gram.strip(\" ,.;:\\(\\)\")\n",
    "                lst_n_grams_in_content.append(n_gram)\n",
    "\n",
    "            for n_gram in lst_n_grams_in_content:\n",
    "\n",
    "                if n_gram.lower() == loc.lower() and loc not in locations_in_content:\n",
    "                    locations_in_content.append(loc)\n",
    "\n",
    "                    \n",
    "                    \n",
    "        dict_books_to_loc[title_book] = locations_in_content\n",
    "        print(content_curtitle)\n",
    "print(dict_books_to_loc)"
   ]
