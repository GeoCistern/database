    "\n",
    "\n",
    "for j in range(len(author_text_file_paths_vol1)):\n",
    "    dict_titles_to_loc = {} # dict (titles : locations) (list has locations of content)\n",
    "      \n",
    "    with open (author_text_file_paths_vol1[j][1]) as f:\n",
    "        str_textfile = f.read()\n",
    "        lst_titles_curauth = re.findall(regex_work_titles, str_textfile)\n",
    "        lst_titles_indices = []\n",
    "        for title in lst_titles_curauth:\n",
    "            lst_titles_indices.append(str_textfile.find(title))\n",
    "\n",
    "        lst_nusha_data = re.findall(regex_nusha, str_textfile)\n",
    "        lst_nusha_indices = []\n",
    "        for nusha in lst_nusha_data:\n",
    "            lst_nusha_indices.append(str_textfile.find(nusha))\n",
    "\n",
    "        lst_baski_data = re.findall(regex_baski, str_textfile)\n",
    "        lst_baski_indices = []\n",
    "        for baski in lst_baski_data:\n",
    "            lst_baski_indices.append(str_textfile.find(baski))\n",
    "\n",
    "        n_i = 0\n",
    "        b_i = 0\n",
    "        #edge cases:\n",
    "        #no nushas or baskis (or just none of 1)\n",
    "        #end of file\n",
    "\n",
    "        for i in range(len(lst_titles_indices)):\n",
    "\n",
    "            bool_nusha_file = True\n",
    "            bool_baski_file = True\n",
    "            content_curtitle = ''\n",
    "            if len(lst_nusha_indices) == 0:\n",
    "                bool_nusha_file = False\n",
    "            if len(lst_baski_indices) == 0:\n",
    "                bool_baski_file = False\n",
    "\n",
    "\n",
    "            # per title\n",
    "            bool_nusha_title = False\n",
    "            bool_baski_title = False\n",
    "            if bool_nusha_file:\n",
    "\n",
    "                while lst_nusha_indices[n_i] < lst_titles_indices[i] and len(lst_nusha_indices) > n_i + 1:\n",
    "                    n_i += 1         \n",
    "            # we are continuing to add all the possibilities per each title\n",
    "            if bool_baski_file:\n",
    "\n",
    "                while lst_baski_indices[b_i] < lst_titles_indices[i] and len(lst_baski_indices) > b_i + 1:\n",
    "                    b_i += 1\n",
    "\n",
    "            if i == len(lst_titles_indices) - 1:\n",
    "                if bool_nusha_file:\n",
    "                    if lst_nusha_indices[n_i] > lst_titles_indices[i]:\n",
    "                        bool_nusha_title = True\n",
    "                if bool_baski_file:\n",
    "                    if lst_baski_indices[b_i] > lst_titles_indices[i]:\n",
    "                        bool_baski_title = True\n",
    "            else:\n",
    "                if bool_nusha_file:\n",
    "                    if lst_nusha_indices[n_i] > lst_titles_indices[i] and lst_nusha_indices[n_i] < lst_titles_indices[i+1]:\n",
    "                        bool_nusha_title = True\n",
    "                        # remove nusha index\n",
    "                if bool_baski_file:\n",
    "                    if lst_baski_indices[b_i] > lst_titles_indices[i] and lst_baski_indices[b_i] < lst_titles_indices[i+1]:\n",
    "                        bool_baski_title = True\n",
    "\n",
    "            # if there are both nusha and baski \n",
    "            if bool_nusha_title and bool_baski_title:\n",
    "                # print(\"both nusha and baski\")\n",
    "                if lst_nusha_indices[n_i] < lst_baski_indices[b_i]:\n",
    "                    content_curtitle = str_textfile[lst_titles_indices[i]:lst_nusha_indices[n_i]]\n",
    "                else:\n",
    "                    content_curtitle = str_textfile[lst_titles_indices[i]:lst_baski_indices[b_i]]\n",
    "\n",
    "            elif not bool_nusha_title and bool_baski_title:\n",
    "                # print(\"only baski\")  \n",
    "                content_curtitle = str_textfile[lst_titles_indices[i]:lst_baski_indices[b_i]]\n",
    "            elif not bool_baski_title and bool_nusha_title:\n",
    "                # print(\"only Nusha\")\n",
    "                content_curtitle = str_textfile[lst_titles_indices[i]:lst_nusha_indices[n_i]]\n",
    "            #no nusha or baski\n",
    "            else:\n",
    "                # print(\"goes here none\")\n",
    "\n",
    "                if i == len(lst_titles_indices) - 1:\n",
    "                    content_curtitle = str_textfile[lst_titles_indices[i]: ]\n",
    "                else:\n",
    "                    content_curtitle = str_textfile[lst_titles_indices[i]:lst_titles_indices[i + 1]]\n",
    "\n",
    "\n",
    "\n",
    "            locations_in_content = []\n",
    "            content_curtitle = lst_titles_curauth[i] + \" \" + content_curtitle\n",
    "            lst_words_in_content = []\n",
    "\n",
    "            # Get content for each title \n",
    "\n",
    "            for loc in loc_and_num_words:\n",
    "                if loc.find(\"-\") != -1:\n",
    "                    lst_words_in_content = content_curtitle.replace('\\t', ' ').replace('\\n', ' ').split()\n",
    "                else:\n",
    "                    lst_words_in_content = content_curtitle.replace('\\t', ' ').replace('\\n', ' ').replace('-', ' ').split()\n",
    "                n_gram_length = loc_and_num_words[loc]\n",
    "                lst_n_grams_in_content = []\n",
    "                for w in range(len(lst_words_in_content) - n_gram_length + 1):\n",
    "                    n_gram = \"\"\n",
    "                    for n in range(n_gram_length):\n",
    "                        n_gram += lst_words_in_content[w + n] + \" \"\n",
    "                    # remove spaces from the end \n",
    "                    n_gram = n_gram.strip(\" ,.;:\\(\\)\")\n",
    "                    lst_n_grams_in_content.append(n_gram)\n",
    "\n",
    "                for n_gram in lst_n_grams_in_content:\n",
    "\n",
    "                    if n_gram.lower() == loc.lower() and loc not in locations_in_content:\n",
